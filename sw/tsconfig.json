{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2015",
    "lib": ["ESNext", "WebWorker"],
    "strict": true,
    "esModuleInterop": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true
  },
  "include": [
    "src/resources.d.ts",
    "src/sw.ts",
    "../shared/version.ts"
  ]
}
